#imported required libraries
from alpha_vantage.timeseries import TimeSeries
import pandas as pd
import os
from dotenv import load_dotenv
from pprint import pprint
#import library using api key from .env
load_dotenv()
api_key = os.getenv("ALPHAVANTAGE_API_KEY")

ts = TimeSeries(key=api_key, output_format='pandas')

# pprint(data.head(2))
tickers = []

# how to loop somethign 5 times
print(("Enter 5 stock tickers that you would like to run and press enter after each stock ticker. type done when you have finished and press enter"))
# range generates numbres from 0 -> 5
while True:
    data = input()
    if str.lower(data) == "done":
        break
    tickers.append(data)


 # For the default date string index behavior

for ticker in tickers:
    ts.get_intraday(symbol=ticker,interval='1min', outputsize='full')

df = pd.DataFrame(data, index=['Stock1','Stock2', 'Stock3', 'Stock4', 'Stock5'], columns=['close'])

print(df)